{
    "name": "mtconsultingroup/canvas-portfolio",
    "description": "A Laravel publishing platform",
    "license": "MIT",
    "authors": [
        {
            "name": "Todd Austin",
            "email": "austin.todd.j@gmail.com"
        },
        {
            "name": "Luca Quagliana",
            "email": "luca.quagliana97@gmail.com"
        },
        {
            "name": "Lorenzo Tosetti",
            "email": "info@tosettil.me"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "ext-json": "*",
        "google/cloud": "^0.152",
        "laravel/framework": "^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "laravel/legacy-factories": "^1.0",
        "mockery/mockery": "^1.4",
        "orchestra/testbench": "^4.0|^5.0|^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Canvas\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Canvas\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Canvas\\CanvasServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
